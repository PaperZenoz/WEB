{% set page = { title: 'Вакансии', background: 'vacancy' } %}
{% extends "./2.parts/main.njk" %}
{% block content %}

    <div class="container">
        <div class="breadcrumbs">
            {% set  list = [
                'Wmodels',
                'Вакансии'] %}

            {% for item in list %}
                <a href="#" class="breadcrumbs__link">
                    {{ item }}
                </a>
            {% endfor %}
        </div>
    </div>



    {% set  _ = 'vacancy' %}
    <div class="{{ _ }} section section--no-top">
        <div class="container">
            <div class="{{ _ }}__wrap">
                <div class="{{ _ }}__title h1">
                    Вебкам модель
                </div>
                <div class="{{ _ }}__item">
                    <div class="{{ _ }}__left">
                        <div class="{{ _ }}__subtitle h2">
                            Мы ищем
                        </div>

                        {% set  list = [
                            'Возраст от 18 до 35 лет',
                            'Приятная внешность',
                            'Эмоциональных и открытых',
                            'Желающих хорошо зарабатывать',
                            'Смелых и активных'

                        ] %}

                        <ul class="ul">

                            {% for item in list %}
                                <li>
                                    {{ item }}
                                </li>
                            {% endfor %}

                        </ul>

                    </div>
                    <div class="{{ _ }}__right">
                        <div class="{{ _ }}__subtitle h2">
                            Вы получаете
                        </div>


                        {% set  list = [
                            'Высокий заработок: от 120 000 ₽',
                            'Комфортный и красивый офис',
                            'Комнаты с дизайнерскими интерьерами',
                            'Работа в активном и целеустремленном коллективе',
                            'Тренера, способные раскрыть потенциал каждой модели'
                        ] %}

                        <ul class="ul">

                            {% for item in list %}
                                <li>
                                    {{ item }}
                                </li>
                            {% endfor %}

                        </ul>

                    </div>
                    <div class="{{ _ }}__img">
                        <img src="images/{{ _ }}/1.png" alt=""/>
                    </div>
                    <div class="{{ _ }}__img-mobile img">
                        <img src="images/{{ _ }}/11.png" alt=""/>
                    </div>
                </div>
                <div class="{{ _ }}__button">
                    <a data-fancybox href="#vacancy-popup" class="btn btn--yellow btn--shadow">Заполнить анкету</a>
                </div>


                <div class="{{ _ }}__title h1">
                    Администратор
                </div>
                <div class="{{ _ }}__item">
                    <div class="{{ _ }}__left">
                        <div class="{{ _ }}__subtitle h2">
                            Мы ищем
                        </div>

                        {% set  list = [
                            'Возраст от 25 до 35 лет',
                            'График работы 5/2. 8-00 - 16-00',
                            'Готовность и любовь  к ранним подъемам',
                            'Комфортный график и возможность выбирать  выходные',
                            'Очень ответвлённую  и осознанную',
                            'Умеющую располагать к себе  и которая всегда находит выход из любой ситуации'


                        ] %}

                        <ul class="ul">

                            {% for item in list %}
                                <li>
                                    {{ item }}
                                </li>
                            {% endfor %}

                        </ul>

                    </div>
                    <div class="{{ _ }}__right">
                        <div class="{{ _ }}__subtitle h2">
                            Задачи
                        </div>


                        {% set  list = [
                            'Контроль рабочих процессов',
                            'Мотивация  и поддержание коллектива',
                            'Быть легкой в общении',
                            'Проявлять инициативу в любых делах студии',
                        ] %}

                        <ul class="ul">

                            {% for item in list %}
                                <li>
                                    {{ item }}
                                </li>
                            {% endfor %}

                        </ul>

                    </div>
                    <div class="{{ _ }}__img">
                        <img src="images/{{ _ }}/2.png" alt=""/>
                    </div>
                    <div class="{{ _ }}__img-mobile img">
                        <img src="images/{{ _ }}/12.png" alt=""/>
                    </div>
                </div>
                <div class="{{ _ }}__button">
                    <a data-fancybox href="#vacancy-popup" class="btn btn--yellow btn--shadow">Заполнить анкету</a>
                </div>


